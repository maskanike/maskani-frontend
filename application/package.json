{
  "name": "maskani-frontend",
  "version": "0.0.1",
  "description": "Maskani Frontend in VueJS",
  "repository": {
    "type": "git",
    "url": "https://github.com/itsmagondu/maskani-frontend"
  },
  "scripts": {
    "generate-build-version": "node generate-build-version",
    "serve": "vue-cli-service serve",
    "build": "yarn run generate-build-version && vue-cli-service build",
    "format": "prettier --write --config .prettierrc \"**/*.js\"",
    "lint": "vue-cli-service lint --fix",
    "test:e2e": "vue-cli-service test:e2e --mode e2e",
    "test:unit": "vue-cli-service test:unit",
    "remark": "remark . -o",
    "ci": "vue-cli-service test:e2e --env API_URL=https://maskani.co.ke,ENV=ci --headless --config video=false",
    "coverage": "nyc report --reporter=text-summary"
  },
  "husky": {
    "hooks": {
      "pre-commit": "yarn lint && yarn remark"
    }
  },
  "dependencies": {
    "@mdi/font": "^5.1.45",
    "axios": "^0.19.2",
    "core-js": "^3.6.4",
    "date-fns": "^2.11.1",
    "remark-cli": "^8.0.0",
    "vee-validate": "^3.3.0",
    "vue": "^2.6.11",
    "vue-analytics": "^5.22.1",
    "vue-country-flag": "^2.0.0",
    "vue-i18n": "^8.17.4",
    "vue-meta": "^2.3.3",
    "vue-resize-text": "^0.1.1",
    "vue-router": "^3.1.6",
    "vuetify": "^2.2.26",
    "vuetify-confirm": "^2.0.2",
    "vuex": "^3.3.0"
  },
  "devDependencies": {
    "@cypress/code-coverage": "^3.6.0",
    "@kazupon/vue-i18n-loader": "^0.5.0",
    "@vue/babel-preset-app": "^4.3.1",
    "@vue/cli-plugin-babel": "^4.3.0",
    "@vue/cli-plugin-e2e-cypress": "^4.3.0",
    "@vue/cli-plugin-eslint": "^4.3.0",
    "@vue/cli-plugin-unit-mocha": "^4.3.0",
    "@vue/cli-service": "^4.3.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/test-utils": "^1.0.0-beta.32",
    "babel-eslint": "^10.1.0",
    "babel-plugin-istanbul": "^6.0.0",
    "chai": "^4.2.0",
    "eslint": "^6.8.0",
    "eslint-config-formidable": "^4.0.0",
    "eslint-plugin-prettier": "^3.1.2",
    "eslint-plugin-vue": "^6.2.2",
    "faker": "^4.1.0",
    "fibers": "^5.0.0",
    "husky": "^4.2.3",
    "istanbul-lib-coverage": "^3.0.0",
    "nyc": "^15.0.1",
    "prettier": "^2.0.5",
    "sass": "^1.26.5",
    "sass-loader": "^8.0.2",
    "stylus": "^0.54.7",
    "stylus-loader": "^3.0.2",
    "vue-cli-plugin-axios": "0.0.4",
    "vue-cli-plugin-i18n": "^1.0.1",
    "vue-cli-plugin-vuetify": "^2.0.5",
    "vue-template-compiler": "^2.6.11",
    "vuetify-loader": "^1.4.3"
  },
  "keywords": [
    "vuejs",
    "vue-router",
    "vuex",
    "vuetify",
    "javascript",
    "i18n",
    "eslint",
    "skeleton",
    "async",
    "starter",
    "webapp",
    "web",
    "app",
    "await",
    "mvp",
    "frontend",
    "testing",
    "prettier",
    "cypress",
    "mocha",
    "chai"
  ]
}
